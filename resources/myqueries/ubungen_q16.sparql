PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX ub:  <http://www.lehigh.edu/~zhp2/2004/0401/univ-bench.owl#>

SELECT ?X
WHERE {
  ?Y ub:name ?X .
  {?Y rdf:type ub:UndergraduateStudent.}
  UNION
  {?Y rdf:type ub:GraduateStudent.}
  FILTER(regex(?X,"^Und"))
}
ORDER BY ?X
LIMIT 10
OFFSET 100
